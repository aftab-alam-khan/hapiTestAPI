{
  "name": "code-test",
  "private": true,
  "version": "1.0.0",
  "description": "Code Test base repository",
  "main": "api.js",
  "scripts": {
    "start": "node server",
    "test": "lab -v test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pagerinc/node-code-test.git"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "node",
    "hapi",
    "recruiting",
    "test"
  ],
  "author": "Pager Engineering <engineering@pager.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/pagerinc/node-code-test/issues"
  },
  "homepage": "https://github.com/pagerinc/node-code-test#readme",
  "dependencies": {
    "@hapi/hapi": "^20.0.0",
    "mongodb": "^3.6.0"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.2",
    "@hapi/lab": "^23.0.0",
    "@semantic-release/git": "^9.0.0"
  },
  "renovate": {
    "extends": [
      "github>pagerinc/renovate-config"
    ],
    "schedule": [
      "at any time"
    ]
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
